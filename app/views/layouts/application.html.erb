<!DOCTYPE html>
<html>
<head>
  <title>Pag4</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<h1 id="cabecera">Ultra Descargas</h1>

<div class="navegacion">
	<ul class="nav nav-tabs">
	  <li class="active"><a href="/">Home</a></li>
	  <li><a href="batman" title="">Batman</a></li>
	  <li><a href="superman" title="">Superman</a></li>
	</ul>
</div>

	<p class="notice"><%= notice %></p>
	<p class="alert"><%= alert %></p>

<div id="login">
		<p>
	<% if user_signed_in? %>
	Bienvenido <strong><%= current_user.email %></strong>
	<!--<%= link_to 'Editar Perfil', edit_user_registration_path, :class => 'nav-link' %>-->
	<%= link_to "Cerrar Session", destroy_user_session_path, method: :delete, :class => 'nav-link' %>
	<% else %>
	<%= link_to 'Registrate', new_user_registration_path, :class => 'nav-link' %>
	<%= link_to 'login', new_user_session_path, :class => 'nav-link' %>
	<% end %>
</div>


<%= yield %>

</body>
</html>
